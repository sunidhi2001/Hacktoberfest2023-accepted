{
  "name": "Password Generator",
  "version": "1.0",
  "manifest_version": 2,
  "content_security_policy": "default-src 'self'; script-src 'self' https://www.gstatic.com https://www.googleapis.com; style-src 'self' 'unsafe-inline'",
  "description": "This extension generates strong, secure passwords and stores them in an encrypted format.",
  "permissions": ["storage"],
  "browser_action": {
    "default_popup": "index.html"
  }
}
